{
  "name": "sjml",
  "displayName": "SJML",
  "description": "Basic Language Support for SJML",
  "version": "1.0.0",
  "publisher": "Hiruja Edurapola",
  "engines": {
    "vscode": "^1.50.0"
  },
  "icon": "icon.png",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "repository": {
    "type": "Git",
    "url": "https://github.com/HirujaEdurapola/SJML-Support"
  },
  "contributes": {
    "snippets": [
      {
        "language": "sjml",
        "path": "./snippets/snippets.code-snippets"
      }
    ],
    "languages": [
      {
        "id": "sjml",
        "aliases": ["SJML", "sjml"],
        "extensions": [".sjml"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sjml",
        "scopeName": "source.sjml",
        "path": "./syntaxes/sjml.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:sjml"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "typescript": "^4.0.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "vscode-languageserver": "9.0.1"
  }
}
